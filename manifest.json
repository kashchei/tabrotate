{"manifest_version": 3, "name": "Tab Rotator Pro", "version": "2.0.0", "description": "Advanced tab rotation with selective control, smart detection, and enhanced features", "permissions": ["tabs", "storage", "alarms", "scripting", "webRequest"], "host_permissions": ["http://*/*", "https://*/*"], "action": {"default_popup": "popup.html", "default_title": "Tab Rotator Pro", "default_icon": {"16": "logo.png", "48": "logo.png", "128": "logo.png"}}, "background": {"service_worker": "service-worker.js"}, "commands": {"toggle-rotation": {"suggested_key": {"default": "Ctrl+Shift+R", "mac": "Command+Shift+R"}, "description": "Toggle tab rotation on/off"}, "next-tab": {"suggested_key": {"default": "Ctrl+Shift+N", "mac": "Command+Shift+N"}, "description": "Rotate to next tab"}}, "icons": {"16": "logo.png", "48": "logo.png", "128": "logo.png"}}